{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "node",
    "jsx": "react",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "sourceMap": true,
    "paths": {
      "react-photographer": ".."
    }
  }
}
